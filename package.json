{
  "name": "@herberttn/bytenode-webpack-plugin",
  "description": "Compile JavaScript into bytecode using bytenode",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:herberttn/bytenode-webpack-plugin"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-s lint build:ts",
    "build:ts": "tsc --build tsconfig.project.json",
    "clean": "rimraf lib **/.eslintcache **/*.log **/*.tsbuildinfo",
    "lint": "eslint . --ext .js,.ts --cache --max-warnings 0",
    "test": "jest --config jest.config.ts --detectOpenHandles"
  },
  "dependencies": {
    "bytenode": "^1.2.1",
    "webpack": "^4.46.0",
    "webpack-virtual-modules": "^0.4.2"
  },
  "devDependencies": {
    "@jest/types": "26.6.2",
    "@types/jest": "26.0.22",
    "@types/webpack": "^4.41.27",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack-merge": "5.7.3"
  },
  "author": {
    "name": "herberttn",
    "url": "https://github.com/herberttn"
  },
  "contributors": [
    {
      "name": "jjeff",
      "url": "https://github.com/jjeff"
    }
  ],
  "homepage": "https://github.com/herberttn/bytenode-webpack-plugin#readme",
  "bugs": {
    "url": "https://github.com/herberttn/bytenode-webpack-plugin/issues"
  },
  "keywords": [
